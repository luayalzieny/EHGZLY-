    <link rel="stylesheet" href="../public/css/colors/color.css">
    <link rel="stylesheet" href="../public/css/new-preloader.css">
    <title> | REGISTER RESTAURANT | </title>
</head>
<body>

  <section class="signUp-page-banner">
      
    <div class="container">
        <div class="row justify-content-center align-items-center">
            <div class="col-md-12 col-sm-12 text-center welcomeTexte mb-2">
                <h1 class="text-light">Make An Account to USe  TAKEAWAY </h1>
                <span class="signup-header-span "> <a class="text-light" href="index.html"> <i class=" fas fa-home mr-2"></i> BACK TO HOME</a></span>
            </div>
            <div class=" signup-buttons  col-lg-7 col-md-10 col-sm-12 text-center">
                <div class="container">
                    <div  class="row ">
                        
                        <div class=" signup-form col-lg-12 text-white text-center mt-3">
                            <div>
                          <a href="index.ejs"><img src="../public/photos/imgs/takeaway-catering-logo-large.png" width="100" height="100" alt=""></a> 
                            </div>

                            <form action="/restaurant" method="post">
                              <div class=" text-center ">  
                                <div class=" form-row mt-3 ">
                                  <!-- ////////////////////////// -->
                                  <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                     <input type="text" class="col-lg-12 p-2" id="validationCustom" name="name" value="" required placeholder="Restaurant name * ">
                                     <%if (error.restaurantName!=0){%>
                                     <p > <%=error.restaurantName %></p>
                                      <%}%>
                                   </div>
                             <!-- ////////////////////////// -->
                      
                                  <!-- ////////////////////////// -->
                                  <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                    <input type="tel" class="col-lg-12 p-2" id="validationCustom" name="restaurantPhone" value="" required placeholder="Restaurant phone *" pattern="[0-1]{2}[0-9]{9}">
                                    <p>format:01xxxxxxxxx</p>
                                     <%if (error.restaurantPhone!=0){%>
                                    <p> <%=error.restaurantPhone %></p>
                                     <%}%>
                                   </div>
                             <!-- ////////////////////////// -->
                      
                             <!-- ////////////////////////// -->
                                 <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                     <input type="text" class="col-lg-12 p-2" id="validationCustom" name="manger" value="" required placeholder="Manager Name *">
                                     <%if (error.manger!=0){%>
                                      <%=error.manger %>
                                      <%}%>
                                   </div>
                             <!-- ////////////////////////// -->
                      
                                   <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                    <input type="tel" class="col-lg-12 p-2" id="validationCustom" name="mangerPhone" value="" required placeholder="Manager Contact phone *"  pattern="[0-1]{2}[0-9]{9}">
                                    <%if (error.mangerPhone!=0){%>
                                     <p> <%=error.mangerPhone %></p>
                                      <%}%>
                                   </div>
                             <!-- ////////////////////////// -->
                    
                                  <!-- ////////////////////////// -->
                                  <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                    <input type="text" class="col-lg-12 p-2" id="validationCustom" name="username" value="" required placeholder="User Name * ">
                                    <%if (error.username!=0){%>
                                    <p>  <%=error.username%></p>
                                      <%}%>
                                  </div>
                            <!-- ////////////////////////// -->
                            
                                    <div class=" col-lg-6 col-md-6 col-sm-12 p-4">                
                                      <input type="email" class="col-lg-12 p-2 " id="validationCustom" name="email" value="" required placeholder="Contact Email *">
                                      <%if (error.email!=0){%>
                                      <p>  <%=error.email %></p>
                                          <%}%>
                                    </div>
                                    <!-- ////////////////////////// -->
                                    <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                      <input type="password" class="col-lg-12 p-2  " id="validationCustom" name="password" value="" required placeholder="password *">
                                      <%if (error.password!=0){%>
                                        <p><%=error.password %></p>
                                        <%}%>
                                    </div>
                                    <!-- ////////////////////////// -->
                                    <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                      <input type="text" class="col-lg-12 p-2  " id="address" name="address" value="" required placeholder="LongLat *">
                                      <p>format:30.2944688, 31.7379577</p>
                                      <%if (error.address!=0){%>
                                        <p><%=error.address %></p>
                                        <%}%>
                                    </div>
                                    <h6 class="p-2"> to find the address with Longitude and Latitude just <strong>RIGHT CLICK</strong> on the location inside <a class="text-danger" href="https://www.google.com/maps/@30.2963555,31.7106506,5150m/data=!3m1!1e3" target="_blank"> Google Map </a> </h6>
                                     <!-- ////////////////////////// -->
                                    <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                      <input type="text" class="col-lg-12 p-2  " id="kitchen" name="kitchen" value="" required placeholder="kitchen *">
                                      <%if (error.kitchen!=0){%>
                                        <p><%=error.kitchen %></p>
                                        <%}%>
                                    </div>
                                </div>
                             <!-- ////////////////////////////////////////// -->
                                    
                    
                            <div class=" text-left col-lg-12 col-md-12 col-sm-12 p-2  ">
                            <input type="checkbox" class=""  value="" required >
                            <span>By Registering You Confirm That You Accept The Terms & Conditions And Privacy Policy</span>
                            </div>
                              </div>
                              <button type="submit" ><a style="color: #eb102d;" >submit</a></button>
                            </form>
                                                     
                        </div>


                    </div>
                </div>
            </div>

        </div>
    </div>


</section>










      <!-- bootstrap -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="../public/js/bootstrap/jquery-3.5.1.js"></script>
  <script src="../public/js/bootstrap/popper.min.js"></script>
  <script src="../public/js/bootstrap/bootstrap.min.js"></script>
  <!-- fontawsome -->
  <script src="../public/js/fontawsome/fontawesome.min.js"></script>
