<section class="signUp-page-banner">
      
        <div class="container">
            <div class="row justify-content-center align-items-center">
                <div class="col-md-12 col-sm-12 text-center welcomeTexte mb-2">
                    <h1 class="text-light">Make An Account To Use TAKEAWAY </h1>
                    <span class="signup-header-span "> <a class="text-light" href="/"> <i class=" fas fa-home mr-2"></i> BACK TO HOME</a></span>
                </div>
                <div class=" signup-buttons  col-lg-7 col-md-10 col-sm-12 text-center">
                    <div class="container">
                        <div  class="row ">
                            
                            <div class=" signup-form col-lg-12 text-white text-center mt-3">
                                <div>
                                  <a href="/"><img src="../public/photos/imgs/logo.PNG" width="190" height="100" alt=""></a> 
                                </div>

                                <form class="form mt-5" action="/signuppage" method="POST">
                                    <div class=" form-row mt-5 ">
                                         <!-- ////////////////////////// -->
                                         <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                            <input type="text" class="col-lg-12 p-2" name="fname" id="validationCustom" value="" required placeholder="First name">
                                            <%if(error){%>
                                              <p class="mt-1"><%=error.Fname%></p>
                                              <%}%>
                                          </div>
                                          
                                    <!-- ////////////////////////// -->

                                         <!-- ////////////////////////// -->
                                         <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                            <input type="text" class="col-lg-12 p-2" name="lname" id="validationCustom" value="" required placeholder="Last name">
                                            <%if(error){%>
                                            <p class="mt-1"><%=error.Lname%></p>
                                            <%}%>
                                          </div>
                                    <!-- ////////////////////////// -->

                                    <!-- ////////////////////////// -->
                                        <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                            <input type="email" class="col-lg-12 p-2" name="email" id="validationCustom" value="" required placeholder="EMAIL">
                                            <%if(error){%>
                                              <p class="mt-1"><%=error.email%></p>
                                              <%}%>
                                          </div>
                                    <!-- ////////////////////////// -->

                                          <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                            <input type="text" class="col-lg-12 p-2" name="number" id="validationCustom" value="" required placeholder="phone">
                                            <%if(error){%>
                                              <p class="mt-1"><%=error.number%></p>
                                              <%}%>
                                          </div>
                                    <!-- ////////////////////////// -->

                                      <div class=" col-lg-6 col-md-6 col-sm-12 p-4  ">
                                        <input type="password" class="col-lg-12 p-2" name="password" id="validationCustom" value="" required placeholder="password">
                                        <%if(error){%>
                                          <p class="mt-1"><%=error.password%></p>
                                          <%}%>
                                      </div>
                                    <!-- ////////////////////////// -->

                                      <!-- <div class=" col-lg-6 col-md-6 col-sm-12 p-4">                
                                        <input type="password" class="col-lg-12 p-2 mb-3" id="validationCustom" value="" required placeholder="retype password">
                                      </div> -->
                
                                    </div>
                                    <!-- ////////////////////////////////////////// -->
                                 
                                    <button type="submit" class="btn btn-danger signup-btun  p-1 col-lg-2 col-md-2 col-sm-2 mb-3" >Submit</button>
                          
                                  </form> 
                                  <span class="mt-3" >Back to <a style="color: #eb102d;" href="loginpage">log in</a></span>
                            </div>


                        </div>
                    </div>
                </div>

            </div>
        </div>
    

</section>
